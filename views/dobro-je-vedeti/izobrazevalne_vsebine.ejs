<% include ../partials/header %>
<% include ../partials/nonindex %>

<%- podstran.vsebina %>

<div class="izobrazevalne_vsebine">
  <% vsebine.forEach(function(vsebina){ %>
    <div class="vnos">
      <div class="naslovna_slika">
        <img src="/files/izobrazevanje/<%= vsebina.naslovna_slika %>" alt="<%= vsebina.naslov %>"/>
      </div>
      <div class="vsebina">
        <h3><%= vsebina.naslov %></h3>
        <p><a href="/files/izobrazevanje/<%= vsebina.datoteka %>" target="_blank">Preberi <i class="fas fa-arrow-circle-right"></i></a></p>
      </div>
    </div>
  <% }); %>
</div>

<% include ../partials/sidebar %>
<% include ../partials/footer %>
