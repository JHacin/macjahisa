        <div class="stellarnav">
          <ul>
            <% nav_kategorije.forEach(function(kategorija){ %>
            <li class="nav_kategorija">
              <a class="<%= kategorija.naslov == 'V novem domu' ? '' : 'kat_toggle' %>" href="<%= kategorija.naslov == 'V novem domu' ? '/v_novem_domu/1' : '#' %>">
                <!-- <%= kategorija.url %> -->
              <%= kategorija.naslov %>
              </a>
                <!-- PODSTRANI -->
                <% if(kategorija.podstrani_length > 1) { %>
                  <ul class="nav_podstrani">
                    <% nav_podstrani.forEach(function(podstran){ %>
                      <% if (podstran.kategorija.equals(kategorija._id) && podstran.objava == "Da") { %>
                        <li class="nav_podstran"><a href="/<%= kategorija.url %>/<%= podstran.url %>"><%= podstran.naslov %></a></li>
                      <% }; %>
                    <% }); %>
                  </ul>
                <% }; %>
            </li>

            <% }); %>
            <li class="search">
              <form action="/search">
                <input type="text" placeholder="Išči..." name="q" id="tipue_search_input" pattern=".{3,}" title="At least 3 characters" required oninvalid="this.setCustomValidity('Prosimo, vnesite iskalno zahtevo.')" oninput="setCustomValidity('')"/>
                 <button type="submit"><i class="fa fa-search" aria-hidden="true"></i></button>
              </form>
            </li>
          </ul>
        </div>

