    <div class="nav">
      <div class="container">
        <div class="mobile_logo">
          <a href="/">
            <img src="/images/logo_mobile.png" />
          </a>
        </div>
        <div class="mobile_top_contact">
          <a href="mailto:info@macjahisa.si"><i class="fa fa-envelope-o" aria-hidden="true"></i> info@macjahisa.si</a><br />
          <a href="tel:040937959"><i class="fa fa-phone" aria-hidden="true"></i> 040 937 959</a>
        </div>
        <div class="stellarnav">
          <ul>
            <% nav_kategorije.forEach(function(kategorija){ %>
            <li class="nav_kategorija">
              <a href="/<%= kategorija.url %>">
              <%= kategorija.naslov %>
              </a>
                <!-- PODSTRANI -->
                <% if(kategorija.podstrani_length > 1) { %>
                  <ul class="nav_podstrani">
                    <% nav_podstrani.forEach(function(podstran){ %>
                      <% if (podstran.kategorija.equals(kategorija._id)) { %>
                        <li class="nav_podstran"><a href="/<%= kategorija.url %>/<%= podstran.url %>"><%= podstran.naslov %></a></li>
                      <% }; %>
                    <% }); %>
                  </ul>
                <% }; %>
            </li>

            <% }); %>
            <li class="search">
              <form>
                <input type="text" placeholder="Išči..." /> <i class="fa fa-search" aria-hidden="true"></i>
              </form>
            </li>
          </ul>
        </div>

      </div>
    </div>
