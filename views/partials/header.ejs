  <body>
    <div class="mobile_header">
      <div class="mobile_logo">
        <a href="/">
          <img src="/images/logo_mobile.png" />
        </a>
      </div>
      <div class="mobile_top_contact">
        <a href="mailto:info@macjahisa.si"><i class="fa fa-envelope-o" aria-hidden="true"></i> info@macjahisa.si</a>
        <a href="tel:040937959"><i class="fa fa-phone" aria-hidden="true"></i> 040 937 959</a>
      </div>
      <div class="mobile_nav">
        <i class="fa fa-bars" aria-hidden="true"></i>
      </div>
    </div>
    <div class="top_bar">
      <div class="container">
        <div class="top_bar_contact">
          <a href="mailto:info@macjahisa.si"><i class="fa fa-envelope-o" aria-hidden="true"></i> info@macjahisa.si</a>
          <a href="tel:040937959"><i class="fa fa-phone" aria-hidden="true"></i> 040 937 959</a>
        </div>
        <div class="top_bar_social">
          <a href="http://www.macjiboter.si/" target="_blank">Mačji boter</a> |
          <a href="http://www.supercombe.si/" target="_blank">Super Čombe</a> |
          <a href="http://www.macjahisa-vet.si/" target="_blank">Veterina MH</a>
          <a href="https://www.facebook.com/MacjaHisa" target="_blank"><i class="fa fa-facebook-official" aria-hidden="true"></i></a>
          <a href="https://twitter.com/MacjaHisa" target="_blank"><i class="fa fa-twitter-square" aria-hidden="true"></i></a>
          <a href="http://www.youtube.com/channel/UC_Gq4voyvfoOCafJMNY0oHg" target="_blank"><i class="fa fa-youtube-square" aria-hidden="true"></i></a>
          <a href="http://instagram.com/macjahisa" target="_blank"><i class="fa fa-instagram" aria-hidden="true"></i></a>
          <a href="https://plus.google.com/+macjahisa" target="_blank"><i class="fa fa-google-plus-square" aria-hidden="true"></i></a>
        </div>
      </div>
    </div>
    <div class="jumbo">
      <div class="container">
        <div class="jumbo_logo">
          <a href="/">
            <img src="/images/logo.png" />
          </a>
        </div>
        <div class="jumbo_links">
          <div class="jumbo_button">Dohodnina <i class="fa fa-caret-right" aria-hidden="true"></i></div>
          <div class="jumbo_button">Posvojitev <i class="fa fa-caret-right" aria-hidden="true"></i></div>
          <div class="jumbo_button">Donacije <i class="fa fa-caret-right" aria-hidden="true"></i></div>
          <div class="jumbo_button">PayPal donacije <i class="fa fa-caret-right" aria-hidden="true"></i></div>
        </div>
      </div>
    </div>
    <div class="nav">
      <div class="container">
        <div class="nav_div">
          <ul class="nav_list">
            <% nav_kategorije.forEach(function(kategorija){ %>
            <li class="nav_category">
              <a href="#">
              <%= kategorija.naslov %>
              <% if(kategorija.podstrani_length > 1) {%>
                <i class="fa fa-caret-down" aria-hidden="true"></i>
              <% }%>
              </a>
            </li>
            <% }); %>
          </ul>
        </div>
        <div class="search">
          <form>
            <input type="text" placeholder="Išči..." /> <i class="fa fa-search" aria-hidden="true"></i>
          </form>
        </div>
      </div>
    </div>
