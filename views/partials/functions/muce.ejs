<%
<!-- STANJE -->
stanjeMuce = function(num) {
  var stanje="";
  if(num==1) {
    stanje="Oddaja se prvič";
  } else if(num==2) {
    stanje="Oddaja se ponovno";
  } else if(num==3) {
    stanje="Se ne oddaja / bo ostala za nedoločen čas";
  } else if(num==4) {
    stanje="V novem domu";
  } else if(num==5) {
    stanje="R.I.P.";
  }
  return stanje;
};

<!-- SPOL -->
spolMuce = function(num) {
  var spol="";
  if(num==1) {
    spol="samček";
  } else if(num==2) {
    spol="samička";
  } else if(num==3) {
    spol="samček in samička";
  } else if(num==4) {
    spol="neznano";
  }
  return spol;
}

<!-- STAROST -->
starostVMesecih = function(mesec_rojstva) {
  var result = Math.floor(moment().diff(moment(mesec_rojstva), "months", true));
  if(result < 1) {
    return "manj kot 1 mesec";
  } else if(result < 12) {
    if(result == 2) {
      return "2 meseca";
    } else if (result == 3 || result == 4) {
      return result + " meseci";
    } else {
      return result + " mesecev";
    }
  } else if (result >= 12) {
    <!-- 1 leto in več -->
    var leta = Math.floor(result/12);
    var meseci = result%12;
    var leta_str = "let";
    var meseci_str = "mesecev";
    <!-- končnica za leta -->
    if(leta == 1) {
      leta_str = "leto";
    } else if (leta == 2) {
      leta_str = "leti";
    } else if (leta == 3 || leta == 4) {
      leta_str = "leta";
    }
    <!-- končnica za mesece -->
    if(meseci == 1) {
      meseci_str = "mesec";
    } else if (meseci == 2) {
      meseci_str = "meseca";
    } else if (meseci == 3 || meseci == 4) {
      meseci_str = "meseci";
    }
    return leta + " " + leta_str + " in " + meseci + " " + meseci_str;
  }
}


%>
